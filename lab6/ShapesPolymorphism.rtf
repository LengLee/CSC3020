    String getName()\{\
        return "Sphere";\
    \}\
    \
    void setRadius(double r)\{\
        super.setDim1(r);\
    \}\
    \
    double getRadius()\{\
        return super.getDim1(); \
    \}\
    \
    @Override \
    double area()\{\
        return (4 * Math.PI * (super.getDim1()*super.getDim1())); \
    \}\
    \
    @Override\
    double volume()\{\
        return ((4/3)*Math.PI * (super.getDim1()*super.getDim1()*super.getDim1())); \
