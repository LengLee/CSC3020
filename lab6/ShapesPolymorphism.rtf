    \
    twoDim()\{\
        super(0.0,0.0);\
        dim1 = dim2 = 0.0; \
    \}\
    \
    twoDim(double a, double b , double c , double d)\{\
        super(a,b);\
        dim1 = c;\
        dim2 = d; \
    \}\
    \
    void setDim1(double a)\{\
        dim1=a;\
    \
    \}\
    void setDim2(double a)\{\
        dim2 = a; \
    \}\
    \
    double getDim1()\{\
        return dim1;\
    \}\
    \
    double getDim2()\{\
        return dim2; \
    \}\
